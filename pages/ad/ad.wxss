    .title{
        width: 100%;
        height: 140rpx;
        background-color: rgb(41, 191, 250);
        text-align: center;
        font-size: 40rpx;
        position: relative;
        color: white;
        line-height: 220rpx;
    }
    .back{
        position: absolute;
        top: 0rpx;
        left: 30rpx;
        height:140rpx
    }
    .write_invate_nums{
        width: 95%;
        height: 120rpx;
        background-color: rgba(240, 248, 255, 0.61);
        margin: 20rpx auto;
        border-radius: 20rpx;
        padding-top: 10rpx;
    }
    .input_of_nums{
        background-color: rgba(255, 84, 84, 0);
