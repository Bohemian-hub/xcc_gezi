        line-height: 80rpx;
    }
    .my_invate{
        width: 95%;
        height: 320rpx;
        background-color: rgba(240, 248, 255, 0.61);
        margin: 20rpx auto;
        border-radius: 20rpx;
    }
    .my_master{
        text-align: center;
        line-height: 100rpx;
    }
    .my_invate_title{
        text-align: center;
        line-height: 100rpx;
    }
    .my_invate_nums{
        text-align: center;
