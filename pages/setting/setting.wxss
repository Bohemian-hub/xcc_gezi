
.title{
    width: 100%;
    height: 140rpx;
    background-color:#f1f1f1;
    text-align: center;
    font-size: 40rpx;
    position: relative;
    color: rgb(97, 97, 97);
    line-height: 220rpx;
}
.back{
    position: absolute;
    top: 0rpx;
    left: 30rpx;
}
.page_tile{
    width: 90%;
    font-size: 60rpx;
    font-weight: 800;
    margin: 0rpx auto 0 auto;
}
.xian_long{
    height: 10rpx;
    width: 80rpx;
    background-color: rgb(39, 101, 235);
    border-radius:5rpx;
    margin-left:40rpx;
    float: left;
}
.xian_short{
    height: 10rpx;
    width: 10rpx;
    background-color: rgb(39, 101, 235);
    border-radius:50%;
    margin-left:5rpx;
    float: left;
}
.clear_both{
    clear: both;
}
.setting_menu{
    width: 40%;
    margin: 30rpx 5% 0 5%;
    height: 300rpx;
    background-color: rgb(255, 255, 255);
    float: left;
    box-shadow: 2rpx 2rpx 20rpx rgb(180, 180, 180);
    text-align: center;
}
.setting_menu_icon{
    width: 60rpx;
    margin-top: 100rpx;
    height: auto;
}
.copyright{
    text-align: center;
    padding-top: 80rpx;
    height: 200rpx;
    font-size: 30rpx;
    color: rgb(131, 131, 131);
}
.loginout{
    width: 90%;
    background-color: rgb(252, 88, 88);
    color: white;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 35rpx;
    margin: 30rpx auto;
    border-radius: 20rpx;
}
